{
  "name": "svelte-kit-ssr-stores-workspace",
  "workspaces": [
    "packages/lib",
    "packages/test-server"
  ],
  "scripts": {
    "build-lib": "npm run build --workspace=svelte-kit-ssr-stores",
    "publish-lib": "npm publish --workspace=svelte-kit-ssr-stores",
    "dev-server": "npm run dev --workspace=test-server",
    "test-server": "npm run start --workspace=test-server",
    "test": "npm run test --workspace=test-server",
    "report": "npm run test --workspace=test-server",
    "record-test": "npm run record-test --workspace=test-server"
  },
  "devDependencies": {
    "prettier": "^2.7.1"
  }
}
